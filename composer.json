{
    "name": "laravel-liberu/avatars",
    "description": "User Avatar manager dependency for Laravel Enso",
    "keywords": [
        "laravel-liberu",
        "laravel",
        "avatars",
        "avatar"
    ],
    "homepage": "https://github.com/laravel-liberu/avatars",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Adrian Ocneanu",
            "email": "aocneanu@gmail.com",
            "homepage": "https://laravel-liberu.com",
            "role": "Developer"
        }
    ],
    "require": {
        "laravel-liberu/core": "^8.0",
        "laravel-liberu/files": "^5.0",
        "laravel-liberu/image-transformer": "^2.0",
        "laravel-liberu/migrator": "^2.0",
        "laravolt/avatar": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelEnso\\Avatars\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelEnso\\Avatars\\AppServiceProvider",
                "LaravelEnso\\Avatars\\AuthServiceProvider"
            ],
            "aliases": {}
        }
    }
}
